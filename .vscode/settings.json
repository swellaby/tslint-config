{
    "files.exclude": {
        "**/.git": true,
        "**/node_modules": true,
        "**/*.tgz": true,
        "**/*.tar*": true,
        "**/.testresults/": true,
    },
    "cSpell.enabled": true,
    "cSpell.enabledLanguageIds": [
        "asciidoc",
        "javascript",
        "json",
        "markdown",
        "plaintext",
        "text",
    ],
    "cSpell.dictionaries": [
      "companies",
      "en_us",
      "filetypes",
      "misc",
      "node",
      "npm",
      "softwareTerms"
    ]
}
